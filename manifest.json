{
  "manifest_version": 2,
  "name": "Priority Changer",
  "version": "1.0.0",
  "description": "Change the priority of your emails...",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}